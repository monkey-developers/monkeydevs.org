<style>
  .right-eye {
    animation: blink 4s infinite forwards;
  }

  .left-eye {
    animation: blink 4s infinite forwards;
  }

  .mkdevs-icon:hover .left-eye {
    animation: move 0.65s linear infinite;
  }

  .mkdevs-icon:hover .right-eye {
    animation: mover 0.65s linear infinite;
  }

  @keyframes move {
    0% {
      left: 20px;
    }
    25% {
      left: 22px;
    }
    50% {
      left: 20px;
    }
    75% {
      left: 19px;
    }
    100% {
      left: 20px;
    }
  }

  @keyframes mover {
    0% {
      left: 36px;
    }
    25% {
      left: 38px;
    }
    50% {
      left: 36px;
    }
    75% {
      left: 35px;
    }
    100% {
      left: 36px;
    }
  }

  @keyframes blink {
    0% {
      top: 36px;
      height: 8px;
    }
    65% {
      top: 36px;
      height: 8px;
    }
    75% {
      top: 40px;
      height: 2px;
    }
    85% {
      top: 36px;
      height: 8px;
    }
  }
</style>
